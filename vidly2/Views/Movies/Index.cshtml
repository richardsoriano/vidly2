
@model IEnumerable<vidly2.Models.Movie>
@{
    ViewData["Title"] = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<h1>Movies</h1>
<table>
    <thead>
    <tr>
        <td>Movie</td>

        </tr>
    </thead>

<tbody>
@foreach (var movie in Model)
{
    <tr><td>@movie.Name</td></tr>
        }
    </tbody>
</table>
